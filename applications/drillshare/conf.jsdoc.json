{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc", "closure"]
    },	
    "source": {
        "include": [ "./src/" ],
	"includePattern": ".+\\.js(doc|x)?$",
        "exclude": [ "node_modules/" ]
    },
    "plugins": ["plugins/markdown", "better-docs/component"],
  	"templates": {
    	  "better-docs": {
      	    "name": "My React components"
    	  }
  	},
    "opts": {
      "destination": "docs",
      "recurse": true,
      "readme": "README.md"
    }
}
